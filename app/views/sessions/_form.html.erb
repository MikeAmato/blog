<!DOCTYPE html>
<html><head>
<%= provide(:title, "Sign In")%>

 <!-- CSS -->
    <link rel="stylesheet" href="assets/stylesheets/session.css.scss">

</head>
<h1 class="center_align">Sign In</h1>

<%# form_for(:user) do |s| %>
<%= form_for(:session, url: sessions_path) do |s| %>

 <%# render 'shared/error_messages', object: @s %> 
  <div>
    <div>
      <%= s.label :email %>
      <%= s.text_field :email %>
    </div>
    <br/>

    <div>
      <%= s.label :password %>
      <%= s.password_field :password%>
    </div>
    <br/>
    
    <div>
      <%= s.submit 'Done' %>
    </div>
  </div>
<% end %> 

